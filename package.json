    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.3",
    "@rushstack/eslint-config": "^3.3.3",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.7",
    "@types/supertest": "^2.0.12",
    "reflect-metadata": "^0.1.13",
    "eslint": "^8.48.0",
    "jest": "^29.6.4",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
