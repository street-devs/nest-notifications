    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "increase-version": "node scripts/increase-version.js"
  },
  "dependencies": {},
  "devDependencies": {
    "@nestjs/core": "^10.2.3",
    "@nestjs/common": "^10.2.3",
